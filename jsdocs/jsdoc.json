{
  "source": {
    "include": ["src"],
    "includePattern": ".js$",
    "excludePattern": ""
  },
  "plugins": ["plugins/markdown"],
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true
  },
  "opts": {
    "recurse": true,
    "destination": "./docs/",
    "template": "custom-template",
    "tutorials": "./tutorials",
    "readme": "./readme.md"
  }
}